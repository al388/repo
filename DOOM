		d
		while True :
			if x == 1 :
				report = r.post(url_Rep, data=self, headers=header)
			elif x == 2 :
				report = r.post(url_Rep, data=hate, headers=header)
			elif x == 3 :
				report = r.post(url_Rep, data=firearms, headers=header)
			elif x == 4 :
				report = r.post(url_Rep, data=spam, headers=header)
			elif x == 5 :
				report = r.post(url_Rep, data=violence, headers=header)
			i=report.json()
			if "ok" in i["status"] :
				done += 1
			else :
				error += 1
			print(f"\r\033[1;30m[\033[0;37m$\033[1;30m] \033[1;32mDONE -{done}- \033[0;37m| \033[1;31mERROR -{error}- \033[0;37m| Target : {Target}",end='')
	elif ("checkpoint_required") or ("challenge_required") in Log.text:
		print("\033[1;30m[\033[0;37m!\033[1;30m] \033[1;31mERROR : Secure")
		exit()
	elif '"two_factor_required":true' in Log.text:
		print("\033[1;30m[\033[0;37m!\033[1;30m] \033[1;31mERROR : 2FA")
		exit()
	else:
		print("\033[1;30m[\033[0;37m!\033[1;30m]\033[1;31m ERROR")
ops()
